// booleans
assert (= true true)
assert (= false false)
assert (= (= true false) false)
assert (&& true true)
assert (! (&& false true))
assert (! (&& true false))
assert (! (&& false false))
assert (|| true true)
assert (|| true false)
assert (|| false true)
assert (! (|| false false))
assert (^^ true false)
assert (^^ false true)
assert (! (^^ true true))
assert (! (^^ false false))
