// floating points
let x = 1.2
let y = 2.0
assert (= (- x) -1.2)
assert (= (+ x y) 3.2)
assert (= (- x y) -0.8)
assert (= (* x y) 2.4)
assert (= (/ x y) 0.6)
assert (= (% y x) 0.8)
assert (= (< x y) true)

// integers
x = 3i
y = 5i
assert (= (- x) -3i)
assert (= (+ x y) 8i)
assert (= (- x y) -2i)
assert (= (* x y) 15i)
assert (= (/ x y) 0i)
assert (= (% y x) 2i)
assert (= (< y x) false)
assert (= (& x y) 1i)
assert (= (| x y) 7i)
assert (= (^ x y) 6i)
assert (= (<< x y) 96i)
assert (= (>> 16i 2i) 4i)
assert (= (>>> -1i 60i) 15i)
